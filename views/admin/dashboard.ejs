<%- include('../partials/header') %> 
<%- include('../partials/navbar') %> 

<div class="container container-dashboard mt-5">
    <div class="row d-flex align-items-center">
        <h1 class="mb-5">Admin Dashboard</h1>
        <% menus.forEach((menu) => { %> 
            <div class="col-md-4 mb-3">
                <div class="card card-dashboard">
                    <div class="card-body card-dashboard-content">
                        <div class="d-flex justify-content-center mb-2">
                            <i class="d-flex justify-content-center <%= menu.logo %>"></i>
                        </div>
                        <div class="d-flex justify-content-center mb-3">
                            <p class="card-text d-flex justify-content-center"><%= menu.text %> </p>
                        </div>
                        <div class="d-flex justify-content-center">
                            <a href="<%= menu.path %>" class="d-flex justify-content-center btn btn-primary">Go</a>
                        </div>
                    </div>
                </div>
            </div>
        <% }) %>
    </div>
</div>

<%- include('../partials/footer') %> 